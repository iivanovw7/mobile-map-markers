        "history": "5.2.0",
        "lodash.debounce": "4.0.8",
        "lodash.throttle": "4.1.1",
        "prop-types": "15.8.0",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-error-boundary": "3.1.4",
        "react-intl": "^5.24.1",
        "react-redux": "7.2.6",
