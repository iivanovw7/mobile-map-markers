{
    "name": "mobile-map-markers",
    "version": "1.0.0",
    "description": "mobile app with map markers",
    "directories": {
        "assets": "assets",
        "config": "config",
        "src": "src",
        "tool": "tool"
    },
    "main": "src/index.jsx",
    "scripts": {
        "build": "run-s \"clean\" \"build:webpack -- {@}\" \"build:gulp:postbuild -- {@}\" --",
        "build:gulp:postbuild": "gulp postbuild",
        "build:webpack": "node ./tool/build.js",
        "coverage": "npm run test -- --coverage",
        "clean": "rm -rf dist",
        "dev": "run-s \"dev:server -- {@}\" --",
        "dev:dashboard": "run-s \"dev:server:dashboard -- {@}\" --",
        "dev:server": "node ./tool/server.js",
        "dev:server:dashboard": "webpack-dashboard -- node ./tool/server.js",
        "doc": "jsdoc -c config/jsdoc/conf.json --private",
        "lint": "run-s \"lint-css\" \"lint-js\" \"lint-eslint\"",
        "lint-css": "stylelint --fix \"./assets/css/**/*.{css, pcss}\"",
        "lint-css:fix": "stylelint \"./assets/css/**/*.{css, pcss}\" --fix",
        "lint-js": "stylelint \"./src/**/*.styled.js\"",
        "lint-eslint": "eslint . --ext .js --ext .jsx --ignore-pattern internals/scripts --ignore-path .eslintignore",
        "lint-eslint:fix": "eslint . --ext .js --ext .jsx --ignore-pattern internals/scripts --ignore-path .eslintignore --fix",
        "reset": "rm -rf dist && rm -rf node_modules && rm package-lock.json",
        "test": "test",
        "view": "node ./tool/view.mjs"
    },
    "author": "iivanovw7",
    "license": "ISC",
    "dependencies": {
        "@react-google-maps/api": "2.7.0",
        "@reduxjs/toolkit": "1.7.1",
